# Configuração para o Render (backend)
NODE_ENV=production
PORT=10000

# Database - usar a URL do Supabase
DATABASE_URL=your_supabase_database_url_here

# JWT Secret - gerar uma chave segura
JWT_SECRET_KEY=your_secure_jwt_secret_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS - permitir domínio do Vercel
FRONTEND_URL=https://your-vercel-app.vercel.app
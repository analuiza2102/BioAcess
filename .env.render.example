# Exemplo de variáveis de ambiente para Render.com
# Configure estas variáveis no painel do Render

# Database (será fornecido automaticamente pelo Render Database)
SUPABASE_DB_URL=postgresql://user:password@host:port/database

# JWT Security
JWT_SECRET=your-super-secret-jwt-key-here-min-32-chars
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=30

# CORS (adicione seu domínio do Render)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-app.onrender.com

# Biometria
EMBEDDING_MODEL=Facenet
SIMILARITY_THRESHOLD=0.6

# Liveness Detection
LIVENESS_EMBEDDING_DIFF_MIN=0.05
LIVENESS_EMBEDDING_DIFF_MAX=0.25

# Demo Mode (set to false in production)
DEMO_MODE=false

# Render específicas
PORT=10000
HOST=0.0.0.0
PYTHON_VERSION=3.11.5